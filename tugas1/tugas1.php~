<?php

class kontak_person {

  // properti
   var $nama;
   var $no_hp;
   var $email;
   var $alamat;
   var $pin_bbm:
}

// buat objek dari class kontak_person
$nama = new kontak_person();


// set property untuk objek bimo
$nama->nama="Bimo Anugrah Prasetyo"
$nama->no_hp="0811111";
$nama->email="bimo@gmail.com";
$nama->alamat="Semarang"
$nama->pin_bbm="123DE"


// Cetak property untuk objek nama
echo "kontak_person";
echo $nama->nama;
echo $nama->no_hp;
echo $nama->email;
echo $nama->alamat;
echo $nama->pin_bbm;



?>
